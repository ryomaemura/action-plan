<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<% if @users %>
  <ul class="users-list">
    <% @users.each do |user| %>
      <li class="user-item">
        <p class="user-name"><%= user.name %></p>
        <p class="user-action-number"></p>
      </li>
    <% end %>
  </ul>
<% end %>
